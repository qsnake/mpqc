
molecule = $:water

% this will give very poor results for water
resolution = 0.5

wavefunction<HCoreWfn>: (
  docc = [ 3 0 1 1 ]

  % Function
  value_accuracy = 1e-9
  gradient_accuracy = 1e-7

  % MolecularEnergy input
  molecule = $:molecule
  basis<GaussianBasisSet>: (
    molecule = $:molecule
    name = "6-31G"
    puream = yes
  )

  % comment out coor if molecule is an atom
  coor = $:coor
)

pwavefunction<HCoreWfn>: (
  docc = [ 3 0 1 1 ]

  % Function
  value_accuracy = 1e-9
  gradient_accuracy = 1e-7

  % MolecularEnergy input
  molecule = $:molecule
  basis<GaussianBasisSet>: (
    molecule = $:molecule
    name = "STO-3G"
  )

  % comment out coor if molecule is an atom
  coor = $:coor
)

coor = $:symcoor
message = $:message1


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
% message types
%

message1<ProcMessageGrp>: ()

messageShm<ShmMessageGrp>: (
  n = 2
)

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
% internal coordinate types
%

redcoor<RedundMolecularCoor>: (
  molecule = $:molecule
)

symcoor<SymmMolecularCoor>: (
  molecule = $:molecule
)

cartcoor<CartMolecularCoor>: (
  molecule = $:molecule
)

