% molecule specification
molecule<Molecule>: (
  symmetry = D2H
  angstroms = yes
  { atoms geometry } = {
     C [  0.0000   1.0094   0.0000 ]
     C [  0.0000  -1.0094   0.0000 ]
     H [  0.9174   1.6662   0.0000 ]
     H [ -0.9174  -1.6662   0.0000 ]
     H [  0.9174  -1.6662   0.0000 ]
     H [ -0.9174   1.6662   0.0000 ]
  }
)
% basis set specification
basis<GaussianBasisSet>: (
  name = "3-21G"
  molecule = $:molecule
)
mpqc: (
  savestate = no
  restart = no
  do_energy = yes
  do_gradient = no
  % method for computing the molecule's energy
  mole<MBPT2>: (
    molecule = $:molecule
    basis = $:basis
    memory = 16000000
    nfzc = 2
    nfzv = 2
    method = zapt
    algorithm = v2
    reference<HSOSHF>: (
      multiplicity = 3
      molecule = $:molecule
      basis = $:basis
      memory = 16000000
    )
  )
)
